{
  "name": "lottienodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "nodemon --delay 100ms --watch . --exec node dist/index.js",
    "serve2": "nodemon --delay 100ms --watch . --exec node ./dist/scripts/export-infographic-test.js",
    "serve-paper": "nodemon --delay 100ms --watch . --exec node index-paper.js",
    "serve-paper-lottie": "nodemon --delay 100ms --watch . --exec node index-paper-lottie.js",
    "serve-node-canvas": "nodemon --delay 100ms --watch . --exec node index-node-canvas.js",
    "serve-fabric-lottie": "nodemon --delay 100ms --watch . --exec node index-fabric-lottie.js",
    "serve-lottie-web": "nodemon --delay 100ms --watch . --exec node index-lottie-web.js",
    "serve-fabric": "nodemon --delay 100ms --watch . --exec node index-fabric2.js",
    "serve:production": "node dist/index.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build": "tsc",
    "watch-build": "tsc -w",
    "test-e2e": "node ./e2e/index.js",
    "lint-full": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "lint": "tsc --noEmit && eslint {_services,scripts,configs}/**/*.{ts,js} --fix",
    "prettier": "prettier --write {_services,scripts,configs}/**/*.{ts,js}",
    "pre-commit": "npm run lint & npm run prettier"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.18.9",
    "fabric": "3.3.2",
    "btoa": "1.2.1",
    "canvg": "2.0.0",
    "lottie-web": "5.5.7",
    "lottie-node": "1.0.0",
    "typescript": "^3.8.3",
    "@types/lodash": "4.14.108",
    "jest": "^25.1.0",
    "ts-jest": "25.2.1",
    "@types/jest": "22.2.3",
    "eslint": "6.8.0",
    "@typescript-eslint/parser":"2.13.0",
    "@typescript-eslint/eslint-plugin": "2.13.0",
    "prettier": "2.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "dotenv": "^6.1.0",
    "axios": "^0.19.1",
    "moment": "^2.22.2",
    "redis": "^2.8.0",
    "rsmq": "^0.9.3",
    "canvas": "^2.6.1",
    "jsdom": "^15.2.1",
    "xmldom": "0.1.27",
    "svg2img": "0.6.1",
    "paper": "^0.12.4"
  }
}